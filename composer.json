{
  "name": "bergwerk/bwrk_utility",
  "description": "BERGWERK utilities for flexform and tca generation",
  "type": "typo3-cms-extension",
  "homepage": "https://www.bergwerk.ag",
  "authors": [
      {
          "name": "BERGWERK",
          "role": "Developer",
          "homepage": "https://www.bergwerk.ag"
      }
  ],
  "license": ["GPL-2.0-or-later"],
  "require": {
    "typo3/cms-core": ">7.6"
  },
  "replace": {
    "bwrk_utility": "self.version",
    "typo3-ter/bwrk_utility": "self.version"
  },
  "autoload": {
    "psr-4": {
      "BERGWERK\\BwrkUtility\\": "Classes"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.0"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}